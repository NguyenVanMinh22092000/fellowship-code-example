{
    "name": "sample-app-service-api",
    "version": "1.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": "18 || 20"
    },
    "scripts": {
        "build": "lb-tsc",
        "build:watch": "lb-tsc --watch",
        "pretest": "yarn run rebuild",
        "preopenapi-spec": "yarn run build",
        "openapi-spec": "node ./dist/openapi-spec",
        "prestart": "yarn run rebuild",
        "docker:build": "docker build -t temp-lb4-with-docker .",
        "docker:run": "docker run -p 3000:3000 -d temp-lb4-with-docker",
        "start": "node -r source-map-support/register .",
        "dev": "yarn run rebuild && tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
        "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
        "rebuild": "yarn run clean && yarn run build",
        "deploy:start": "pm2 start ecosystem.config.js --env production",
        "deploy:stop": "pm2 stop ecosystem.config.js --env production"
    },
    "author": "Kds <minhnguyenkhoi@gmail.com>",
    "license": "ISC",
    "files": [
        "README.md",
        "dist",
        "src"
    ],
    "dependencies": {
        "@loopback/boot": "^7.0.0",
        "@loopback/core": "^6.0.0",
        "@loopback/repository": "^7.0.0",
        "@loopback/rest": "^14.0.0",
        "@loopback/rest-explorer": "^7.0.0",
        "@loopback/service-proxy": "^7.0.0",
        "agenda": "^5.0.0",
        "axios": "^1.6.8",
        "cors": "^2.8.5",
        "crypto-js": "^4.2.0",
        "date-fns": "^3.6.0",
        "exceljs": "^4.4.0",
        "express": "^4.19.1",
        "flatted": "^3.3.1",
        "fs-extra": "^11.2.0",
        "generate-password": "^1.7.1",
        "imagemagick": "^0.1.3",
        "js-base64": "^3.7.7",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.45",
        "mongoose": "^8.2.3",
        "mongoose-long": "^0.8.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.13",
        "object-sizeof": "^2.6.4",
        "pm2": "^5.3.1",
        "tslib": "^2.6.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@loopback/build": "^11.0.0",
        "@loopback/testlab": "^7.0.0",
        "@types/crypto-js": "^4.2.2",
        "@types/express-serve-static-core": "^4.17.43",
        "@types/generate-password": "^1.5.1",
        "@types/imagemagick": "^0.0.35",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/lodash": "^4.17.0",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.11.30",
        "@types/nodemailer": "^6.4.14",
        "@types/uuid": "^9.0.8",
        "source-map-support": "^0.5.21",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.4.3"
    }
}
